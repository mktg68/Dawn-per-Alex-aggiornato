        "0e7b7b80-48aa-4b22-9176-0ad3b4a83706": {
          "type": "faq",
          "settings": {
            "checkbox_expanded": false,
            "title": "What is AgroEcology?",
            "content": "<p> <a href=\"https:\/\/www.agroecology-europe.org\" target=\"_blank\" title=\"AgroEcology in Europe website\"><strong>AgroEcology<\/strong><\/a> is a set of practices and methods, of agricultural production but also of consumption, sustainable for the ecosystem (soil, aquifers, pollinating insects, etc.) for the protection of the environment and the nutritional quality of the products<\/p>"
          }
        }
